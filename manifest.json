{
  "manifest_version": 3,
  "name": "funian",
  "version": "1.0",
  "description": "extra fancy chrome extension by Ian and Funan",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["./scripts/content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "assets/zoof-icon-min.png"
  },
  "web_accessible_resources": [
    {
      "matches": ["https://*/*"],
      "resources": [
        "assets/zoof-icon-min.png",
        "assets/walkingV2.png",
        "tunnel.gif"
      ]
    }
  ]
}
